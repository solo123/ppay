div class='content content_import'
  h1 imp_qf_customer

  div class='row'
    div class='col-md-6'
      == link_to '返回首页', upload_report_check_path, class: "btn btn-danger m-r-10 m-b-10" 
      == link_to '开始导入数据', upload_report_import_path, class: "btn btn-primary m-r-10 m-b-10"
      == link_to '查看导入日志', '#imp_log_id', class: 'btn btn-info m-r-10 m-b-10'
      == link_to '查看操作记录', '#imp_op_log_id', class: 'btn btn-success m-r-10 m-b-10'
      imp_log_id
      imp_op_log_id
      p import loading

    div class='col-md-6'
      = search_form_for @q, url: upload_report_check_path(:query_data_type => 'imp_qf_customer') ,class: 'form-inline' do |f|
        div class='form-group m-r-10'
          = f.search_field :sj_cont, class: 'form-control',placeholder:'请输入入网电话'
        div class='form-group m-r-10'
          = f.search_field :ssid, class: 'form-control',placeholder:'请输入商户id'
        = f.submit '查找',  class: 'form-control btn-warning'
    

        
  div class='panel panel-inverse panel-danger'
    div class='panel-heading'
      div class='panel-heading-btn'
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
      h4 class='panel-title' check
    div class='panel-body'
      div id='data-table_wrapper' class='dataTables_wrapper form-inline dt-bootstrap'
        div class='row'
          div class='col-md-12'
            = paginate @imp_log, :param_name => 'imp_log_page'
        div style='overflow:scroll; height:350px;'
          table class='table' id='imp_log_id'
            thead
              tr
                th open_file_status
                th load_file_status
                th envelop_uid
                th envelop_title
                th envelop_time
                th process_status
                th detail
                th datetime_flag
                
            tbody
            - @imp_log.each do |log|
              tr
                td #{log.open_file_status}
                td #{log.load_file_status}
                th #{log.envelop_uid}
                th #{log.envelop_title}
                th #{log.envelop_time}
                th #{log.process_status}
                th #{log.detail}
                th #{log.datetime_flag}
                  


